\newcommand{\algoendfloat}{% use as a \begin{algoendfloat} environment to start scanning of line
%  \immediate\openout\@mainfff\jobname.fff%
  \efloat@condopen{fff}
  \efloat@iwrite{fff}{\string\begin{\algocf@envname}}%
    \if@domarkers%
